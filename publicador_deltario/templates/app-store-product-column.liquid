{% layout 'layout' %}
{% block title %}Product Column{% endblock %}

{% block breadcrumb %}
<li><a href="index.html">Home</a></li>
<li>App Templates</li>
<li>eCommerce</li>
<li class="active"><a href="app-store-product-column.html">Product Column</a></li>
{% endblock %}

{% block styles %}
{% endblock %}

{% block scripts %}
{% endblock %}

{% block content %}
<div class="store">
	<div class="row">
		<div class="col-sm-4">
			<div class="box box-col">
				<h3 class="product-name"><a href="#">Razer Deathadder Black Edition</a></h3>
				<img src="{{'demo/images/dabe.jpg' | asset_url | img_loc }}" class="img-responsive">
					
				
				<div class="product-footer clearfix">
					<h3 class="mt0 mb0"><span class="product-price label label-danger pull-left">$199.00</span></h3>
					<a href="#" class="btn btn-sm btn-default pull-right add-to-cart"><i class="fa fa-shopping-cart"></i> <span class="hidden-xs">Add to cart</span></a>
				</div>
			</div>
		</div>
		<div class="col-sm-4">
			<div class="box box-col">
				<h3 class="product-name"><a href="#">Razer Naga</a></h3>
				<img src="{{'demo/images/naga.jpg' | asset_url | img_loc }}" class="img-responsive">
					
				
				<div class="product-footer clearfix">
					<h3 class="mt0 mb0"><span class="product-price label label-danger pull-left">$299.00</span></h3>
					<a href="#" class="btn btn-sm btn-default pull-right add-to-cart"><i class="fa fa-shopping-cart"></i> <span class="hidden-xs">Add to cart</span></a>
				</div>
			</div>
		</div>

		
	</div>

</div> 
{% endblock %}
