{% include header %}

	<body class="fixed-header">
		{% include analytics %}
		{% include headerbar %}
		{% include topnav %}
		
		<div id="layout-fixed">
			<div class="ui-layout-west fixed-sidebar-wrapper sidebar-inverse scroll-pane">
				<div class="fixed-sidebar scroll-content">
					{% include sidebar %}
				</div>
			</div>
			<div class="ui-layout-center scroll-pane">
				<div class="fixed-content-wrapper scroll-content">
					<div class="fixed-content">
				        <div class="page-content">
				            <div class="page-heading">            
				                <h1>{% block title %}Blank{% endblock %}</h1>
				                {% include header-options-default %}
				            </div>
				            <ol class="breadcrumb">
				                {% block breadcrumb %}<li><a href="index.html">Home</a></li>{% endblock %}
				            </ol>
				            <div class="container-fluid">
				                {% block content %}{% endblock %}
				            </div> <!-- .container-fluid -->
				        </div> <!-- #page-content -->
				    </div>
					{% include footer %}
				</div>
			</div>

			<div class="ui-layout-east infobar-wrapper scroll-pane">
				{% include rightbar %}
			</div>
		</div>

		{% include scripts %}
		{% include demo-switcher %}
		{{ "js/jquery.layout.min.js" | asset_url | script_tag }}
		
		{% block scripts %}{% endblock %}
	</body>
</html>